{% extends "logged_in_base.json" %}
{% block content %}
    "TweetBox": {
        "tweet_box": {
            "props": {
                "loc": "#tweet-box-home-timeline"
            }
        },
        "tweet_button": {
            "props": {
                "loc":" #timeline div.tweet-button > button > span.button-text.tweeting-text",
                "visible" :false
            }
        },
        "retweet_buttons": {
            "props": {
                "loc": ".Icon.Icon--retweet",
                "list": true
            }
        }
    },
    "HomeStream": {
        "retweet_buttons": {
            "props": {
                "cssloc": ".Icon.Icon--retweet",
                "xloc":"//*[@data-original-title='Retweet']//*[@class='Icon Icon--retweet']",
                "loc":"//*[@class='Icon Icon--retweet']",
                "list": true
            }
        }
    },
    "RetweetDialogBox": {
        "retweet_button": {
            "props": {
                "loc": "#retweet-tweet-dialog-dialog .tweet-button"
            }
        }
    },
{% endblock %}
